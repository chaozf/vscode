    "version": "0.2.0",
    "configurations": [
           
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Octave Program",
            "program": "${workspaceFolder}/${fileBasenameNoExtension}.m", //调试程序文件
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}",
            "autoTerminate": true
        },
        


        {
            "name": "Python: 当前文件",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}